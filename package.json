{
  "name": "project-time-tracker",
  "version": "1.0.0",
  "description": "Time tracking application",
  "main": "./src/index.jsx",
  "scripts": {
    "clean": "rimraf ./dist/*",
    "start": "npm run clean && cross-env NODE_ENV=development webpack-dev-server -d --history-api-fallback --progress --colors",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "pretest": "npm run clean && cross-env NODE_ENV=testing webpack test/tests.mocha.js --output-filename tmpTestBundle.js --target node",
    "lint": "cross-env NODE_ENV=development eslint --ext .js --ext .jsx src/ || exit 0",
    "test": "mocha ./dist/tmpTestBundle.js",
    "pretest:browser": "opener http://localhost:8080/webpack-dev-server/testBundle",
    "test:browser": "npm run clean && cross-env NODE_ENV=testing webpack-dev-server -d --progress mocha!./test/tests.mocha.js --output-filename testBundle.js --colors"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Geril/project-time-tracker.git"
  },
  "author": "Geril",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Geril/project-time-tracker/issues"
  },
  "homepage": "https://github.com/Geril/project-time-tracker#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "deep-freeze": "0.0.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.0.0",
    "node-sass": "^4.1.1",
    "null-loader": "^0.1.1",
    "opener": "^1.4.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "rimraf": "^2.5.4",
    "redux-mock-store": "0.0.6"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.13.0",
    "react": "^15.4.1",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-thunk": "^2.1.0"
  }
}
